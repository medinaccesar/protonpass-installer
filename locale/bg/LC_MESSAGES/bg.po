msgstr "{} Изтегляне на {}..."

msgid "{} Progreso: {percent:.1f}%"
msgstr "{} Напредък: {percent:.1f}%"

msgid "{} Descarga finalizada: {}"
msgstr "{} Изтеглянето приключи: {}"

msgid "Error al descargar el archivo: {}"
msgstr "Грешка при изтегляне на файла: {}"

msgid "{} Calculando la suma de verificación (SHA512)..."
msgstr "{} Изчисляване на контролна сума (SHA512)..."

msgid "{} Suma de verificación esperada: {}"
msgstr "{} Очаквана контролна сума: {}"

